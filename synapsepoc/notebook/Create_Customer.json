				"source": [
					"customer = raw_cust.select('CustomerId','Age','ZipCode','FirstName','LastName','DateOfBirth','Email','Address','Gender')\r\n",
					"finalCustomer = customer.withColumn (\r\n",
					"    \"Location-ID\",\r\n",
					"    F.array(\r\n",
					"        F.lit(\"location-id-1\"),\r\n",
					"        F.lit(\"location-id-2\"),\r\n",
